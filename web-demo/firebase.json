{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "main.js",
      "package.json",
      "package-lock.json",
      "*.config.js"
    ],
    "headers": [{
      "source" : "**/*.@(bin|json)",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "max-age=31536000"
      }]
    }]
  }
}
